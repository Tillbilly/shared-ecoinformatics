<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		
		<bean id="idWriter" class="au.edu.aekos.shared.service.index.IdValueWriter" />
		<bean id="doiWriter" class="au.edu.aekos.shared.service.index.DoiValueWriter" />
		<bean id="descriptionWriter" class="au.edu.aekos.shared.service.index.DescriptionWriter" >
		    <!-- Add description config here -->
		    <property name="abstractTag" value="SHD.datasetAbstract" />
		</bean>
		<bean id="methodWriter" class="au.edu.aekos.shared.service.index.MethodValueWriter" />
		<bean id="studyLocationWriter" class="au.edu.aekos.shared.service.index.StudyLocationWriter" >
		    <property name="convexHullFieldName" value="wktConvexHull" />
		</bean>
		<bean id="studyAreaGeometryWriter" class="au.edu.aekos.shared.service.index.StudyAreaGeometryWriter" >
		    <property name="convexHullFieldName" value="wktConvexHull" />
		</bean>
		
		<!-- Used to process normal field writing along with a custom writer, for a given shared metatag.  Used in a MultiWriter -->
		<bean id="defaultWriter" class="au.edu.aekos.shared.service.index.DefaultValueWriter" />
		
		<bean id="imageLinkWriter" class="au.edu.aekos.shared.service.index.ImageLinkWriter" />
		
		
		<!-- ************* Flora Species File Index Field Writer 'floraFileWriter' ************** -->
		
		<bean id="floraSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SpeciesFileWriter" >
		    <property name="traitName" value="taxon names" />
		</bean>
		<bean id="floraSemanticSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SemanticSpeciesFileWriter" >
		    <property name="traitName" value="taxon names" />
		</bean>
		<bean id="floraFileWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="floraSpeciesFileWriter"/>
		           <ref bean="floraSemanticSpeciesFileWriter"/>
		        </list>
		     </property>
	     </bean>
	     
	     <!-- ************  Flora Species Picked  -->
	     <bean id="floraSemanticSpeciesWriter" class="au.edu.aekos.shared.service.index.SemanticSpeciesWriter">
	         <property name="traitName" value="taxon names" />
	     </bean>
	     <bean id="floraWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="defaultWriter"/>
		           <ref bean="floraSemanticSpeciesWriter"/>
		        </list>
		     </property>
	     </bean>
	     
	     <!-- ************* Flora Common Names File Index Field Writer 'floraCommonFileWriter' ************** -->
  	     
		<bean id="floraCommonSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SpeciesFileWriter" >
		    <property name="traitName" value="common flora names" />
		</bean>
		<bean id="floraCommonSpeciesSemanticFileWriter" class="au.edu.aekos.shared.service.index.CommonSemanticSpeciesFileWriter">
		    <property name="traitName" value="common flora names" />
		    <property name="scientificNameTrait" value="taxon names" />
		</bean>
		<bean id="floraCommonFileWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="floraCommonSpeciesFileWriter"/>
		           <ref bean="floraCommonSpeciesSemanticFileWriter"/>
		        </list>
		    </property>
		</bean>
		
		<bean id="floraCommonSpeciesSemanticWriter" class="au.edu.aekos.shared.service.index.SemanticCommonSpeciesNameWriter">
		    <property name="traitName" value="common flora names" />
		    <property name="scientificTraitName" value="taxon names" />
		</bean>
		<bean id="floraCommonWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="defaultWriter"/>
		           <ref bean="floraCommonSpeciesSemanticWriter"/>
		        </list>
		     </property>
	     </bean>
		
		
		<!-- *************** Fauna Species File Index Field Writer 'faunaFileWriter' ************** -->
		
		<bean id="faunaSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SpeciesFileWriter"  >
		    <property name="traitName" value="fauna taxon names" />
		</bean>
		<bean id="faunaSemanticSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SemanticSpeciesFileWriter" >
		    <property name="traitName" value="fauna taxon names" />
		</bean>
		<bean id="faunaFileWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="faunaSpeciesFileWriter"/>
		           <ref bean="faunaSemanticSpeciesFileWriter"/>
		        </list>
		     </property>
	     </bean>
	     
	     <bean id="faunaSemanticSpeciesWriter" class="au.edu.aekos.shared.service.index.SemanticSpeciesWriter">
	         <property name="traitName" value="fauna taxon names" />
	     </bean>
	     <bean id="faunaWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="defaultWriter"/>
		           <ref bean="faunaSemanticSpeciesWriter"/>
		        </list>
		     </property>
	     </bean>
	     
		
		<!-- ************* Fauna Common Names File Index Field Writer 'faunaCommonFileWriter' ************** -->
		<bean id="faunaCommonSpeciesFileWriter" class="au.edu.aekos.shared.service.index.SpeciesFileWriter"  >
		    <property name="traitName" value="common fauna names" />
		</bean>
		<bean id="faunaCommonSpeciesSemanticFileWriter" class="au.edu.aekos.shared.service.index.CommonSemanticSpeciesFileWriter">
		    <property name="traitName" value="common fauna names" />
		    <property name="scientificNameTrait" value="fauna taxon names" />
		</bean>
		<bean id="faunaCommonFileWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="faunaCommonSpeciesFileWriter"/>
		           <ref bean="faunaCommonSpeciesSemanticFileWriter"/>
		        </list>
		    </property>
		</bean>
		
		<bean id="faunaCommonSpeciesSemanticWriter" class="au.edu.aekos.shared.service.index.SemanticCommonSpeciesNameWriter">
		    <property name="traitName" value="common fauna names" />
		    <property name="scientificTraitName" value="fauna taxon names" />
		</bean>
		<bean id="faunaCommonWriter" class="au.edu.aekos.shared.service.index.MultiWriter">
		    <property name="writerList">
		        <list>
		           <ref bean="defaultWriter"/>
		           <ref bean="faunaCommonSpeciesSemanticWriter"/>
		        </list>
		     </property>
	     </bean>
		
		<bean id="authorWriter" class="au.edu.aekos.shared.service.index.AuthorWriter" >
		    <property name="givenNamesTag" value="SHD.authorGivenNames" />
		    <property name="surnameTag" value="SHD.authorSurname" />
		</bean>
		<bean id="studyAreaBoundingBoxWriter" class="au.edu.aekos.shared.service.index.StudyAreaBoundingBoxWriter" >
		    <property name="minXTag" value="SHD.studyAreaBoundingBoxXmin" />
		    <property name="minYTag" value="SHD.studyAreaBoundingBoxYmin" />
		    <property name="maxXTag" value="SHD.studyAreaBoundingBoxXmax" />
		    <property name="maxYTag" value="SHD.studyAreaBoundingBoxYmax" />
		    <property name="crsTag" value="SHD.studyAreaBoundingBoxCRS" />
		</bean>
		
		<bean id="pubindexConfig" class="au.edu.aekos.shared.service.index.SubmissionIndexConfig">
		    <property name="solrUrl" value="${solr.pubindex.url}" />
		    <property name="authRequired" value="true" />
		    <property name="username" value="${solr.pubindex.username}" />
		    <property name="password" value="${solr.pubindex.password}" />
            <!-- These properties are used for an embedded solr server (i.e. for dev) -->
            <property name="embeddedServer" value="false" />
            <property name="coreName" value="" />
            <property name="embeddedIndexLocation" value="" />
		    <property name="manualFieldWriterMap">
		        <map>
                    <entry key="id" value-ref="idWriter"/>
                    <entry key="doi" value-ref="doiWriter"/>
                    <entry key="description" value-ref="descriptionWriter"/>
                    <entry key="author" value-ref="authorWriter" />
                    <entry key="bbox" value-ref="studyAreaBoundingBoxWriter" />
                    <entry key="SHD.methodName" value-ref="methodWriter"/>
                    <entry key="SHD.methodAbstract" value-ref="methodWriter"/>
                    <entry key="SHD.studySpeciesFloraFile" value-ref="floraFileWriter"/>
                    <entry key="SHD.studySpeciesFloraCommonFile" value-ref="floraCommonFileWriter"/>
                    <entry key="SHD.studySpeciesFaunaFile" value-ref="faunaFileWriter"/>
                    <entry key="SHD.studySpeciesFaunaCommonFile" value-ref="faunaCommonFileWriter"/>
                    <entry key="SHD.taxonName" value-ref="floraWriter"/>
                    <entry key="SHD.floraCommonName" value-ref="floraCommonWriter"/>
                    <entry key="SHD.faunaTaxonomicName" value-ref="faunaWriter"/>
                    <entry key="SHD.faunaCommonName" value-ref="faunaCommonWriter"/>
                    <entry key="SHD.studyLocationFile" value-ref="studyLocationWriter"/>
                    <entry key="SHD.studyAreaGeometry" value-ref="studyAreaGeometryWriter"/>
                    <entry key="SHD.imageFile" value-ref="imageLinkWriter" />
		        </map>
		    </property>
		    
		    <property name="fieldMappingList">
		        <list>
		            <bean class="au.edu.aekos.shared.service.index.SubmissionIndexField">
		                <property name="sharedTag" value="id" />
		                <property name="indexFieldName" value="id" />
		            </bean>
		            <bean class="au.edu.aekos.shared.service.index.SubmissionIndexField">
		                <property name="sharedTag" value="doi" />
